{
  "name": "@cryptobot/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types and interfaces for CryptoBot",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "cd ../../../ && npx jest --testPathPattern=\"packages/@cryptobot/types\"",
    "test:watch": "cd ../../../ && npx jest --watch --testPathPattern=\"packages/@cryptobot/types\"",
    "test:coverage": "cd ../../../ && npx jest --coverage --testPathPattern=\"packages/@cryptobot/types\""
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.5",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}